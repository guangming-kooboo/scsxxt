@model ServicePlatform.Areas.App.ViewModel.Home.Main_M

<!DOCTYPE html>
<html>
<head>
    <title>Bootslider - Responsive Bootstrap CSS3 Slider</title>

    <meta name="viewport" content="width=device-width, height=device-height, initial-scale=1, maximum-scale=1, user-scalable=no" />@*控制页面不能放大*@

    <meta name="description" content="Bootslider is the ultimate premium Bootstrap Slider Plugin offering the capability to show images, videos, html markup and captions paired with modern and fancy 3D transitions. Even more important, it is fully responsive and mobile optimized and can take on any dimensions." />

    <meta name="keywords" content="bootslider, bootstrap, bootstrap slider, fullscreen, gallery, jquery, jquery slider, premium, responsive, slider, swipe, touch, twitter, videos" />

    <meta charset="UTF-8">

    <!-- Bootstrap Family Styles -->
    <link href="css/boot1.css" rel="stylesheet" type="text/css" />
    <link href="css/boot2.css" rel="stylesheet" type="text/css" />
    <link href="css/boot3.css" rel="stylesheet" type="text/css" />
    <link href='css/boot4.css' rel='stylesheet' type='text/css'>

    <link href="~/Areas/App/Content/CSS/boot1.css" rel="stylesheet" type="text/css" />
    <link href="~/Areas/App/Content/CSS/boot2.css" rel="stylesheet" type="text/css" />
    <link href="~/Areas/App/Content/CSS/boot3.css" rel="stylesheet" type="text/css" />
    <link href="~/Areas/App/Content/CSS/boot4.css" rel="stylesheet" type="text/css" />

    <!-- Required Bootslider Styles -->

    <link href="~/Areas/App/Content/CSS/animate.css" rel="stylesheet" type="text/css" />
    <link href="~/Areas/App/Content/CSS/bootslider.css" rel="stylesheet" type="text/css" />
    <link href="~/Areas/App/Content/CSS/css.css" rel="stylesheet" type="text/css" />

    <!-- Page Level Styles -->
    <link href="~/Areas/App/Content/CSS/Page.css" rel="stylesheet" type="text/css" />

</head>
<body>
    <div class="bootslider" id="bootslider">
        <!-- Bootslider Loader -->
        <div class="bs-loader">
            <img src="~/Areas/App/Content/Image/loader.gif" width="31" height="31" alt="Loading.." id="loader" />

        </div>
        <!-- /Bootslider Loader -->
        <!-- Bootslider Container -->
        <div class="bs-container">

            <!-- Bootslider Slide -->
            <div class="bs-slide active" data-animate-in="flipInX" data-animate-out="holeOut">
                <div class="bs-foreground">
                    <div class="container">
                        <div class="row">
                            <div class="col-md-12 bs-vertical-center">
                                <div data-animate-in="fadeInLeft" data-animate-out="fadeOutUpBig" data-delay="1600">
                                    <h1 class="heading">
                                        SHERATON
                                    </h1>
                                    <h2 class="subheading text-white">
                                        喜来登
                                    </h2>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="bs-background">
                    <img src="~/Areas/App/Content/Image/slide-4.jpg" alt="" />
                </div>
            </div>
            <!-- /Bootslider Slide -->
            <!-- Bootslider Slide -->
            <div class="bs-slide" data-animate-in="bounceInUp" data-animate-out="hinge">
                <div class="bs-foreground">
                    <div class="container">
                        <div class="row" data-animate-in="fadeInLeftBig" data-delay="1000" data-animate-out="fadeOutDown">
                            <div class="col-md-12">
                                <h1 class="heading">SHERATON</h1>
                                <p class="subheading text-white"></p>
                                <p>
                                    <a class="btn btn-primary" href="#">GO NOW</a>
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="bs-background">
                    <img src="~/Areas/App/Content/Image/fixedheight-slide-1.jpg" alt="" />
                </div>
            </div>
            <!-- /Bootslider Slide -->
            <!-- Bootslider Slide -->
            <div class="bs-slide" data-animate-in="bounceInDown" data-animate-out="flipOutX">
                <div class="bs-foreground">
                    <div class="container">
                        <div class="row">
                            <div class="col-md-12 text-center">
                                <div data-animate-in="tada" data-animate-out="fadeOutUpBig" data-delay="1000">
                                    <h1 class="heading">
                                        SHERATON
                                    </h1>
                                    <h3 class="subheading text-white">
                                        世界500强喜达屋饭店及度假村集团旗下品牌
                                    </h3>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="bs-background">
                    <img src="~/Areas/App/Content/Image/fixedheight-slide-2.jpg" alt="" />
                </div>
            </div>
            <!-- /Bootslider Slide -->
            <!-- Bootslider Slide -->
            <div class="bs-slide" data-animate-in="rollIn" data-animate-out="flipOutX">
                <div class="bs-foreground">
                    <div class="container">
                        <div class="row">
                            <div class="col-md-12">
                                <h1 class="heading"
                                    data-animate-in="fadeInLeftBig" data-animate-out="fadeOutLeftBig" data-delay="800">
                                    NOW LAYERED
                                </h1>
                                <h2 class="subheading text-white"
                                    data-animate-in="fadeInLeftBig" data-animate-out="fadeOutLeftBig" data-delay="1200">
                                    AND EVEN MORE AWESOME
                                </h2>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="bs-background">
                    <img src="~/Areas/App/Content/Image/fixedheight-slide-3.jpg" alt="" />
                </div>
            </div>
            <!-- /Bootslider Slide -->
            <!-- Bootslider Slide -->
            <div class="bs-slide" data-animate-in="scaleUpIn" data-animate-out="scaleUpOut">
                <div class="bs-foreground">
                    <div class="row">
                        <div class="col-md-6 col-md-offset-3 bs-vertical-center">
                            <div class="bs-video black" data-animate-in="fadeIn" data-delay="800" data-animate-out="fadeOutLeftBig">
                                <iframe src="" width="500" height="285" frameborder="0"  allowFullScreen></iframe>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="bs-background">
                    <img src="~/Areas/App/Content/Image/slide-5.jpg" alt="" />
                </div>
            </div>
            <!-- /Bootslider Slide -->
            <!-- Bootslider Slide -->
            <div class="bs-slide" data-animate-in="openDownLeftReturn" data-animate-out="slideUp">
                <div class="bs-foreground">
                    <div class="bs-video-fullscreen" data-animate-out="fadeOut">
                        <iframe src="#" width="1440" height="720" frameborder="0" allowfullscreen></iframe>
                    </div>
                </div>
                <div class="bs-background">
                    <img src="~/Areas/App/Content/Image/slide-6.jpg" alt="" />
                </div>
            </div>
            <!-- /Bootslider Slide -->

        </div>
        <!-- /Bootslider Container -->
        <!-- Bootslider Progress -->
        <div class="bs-progress progress">
            <div class="progress-bar progress-primary"></div>
        </div>
        <!-- /Bootslider Progress -->
        <!-- Bootslider Thumbnails -->
        <div class="bs-thumbnails text-center text-alizarin">
            <ul class=""></ul>
        </div>
        <!-- /Bootslider Thumbnails -->
        <!-- Bootslider Pagination -->
        <div class="bs-pagination text-center text-alizarin">
            <ul class="pagination"></ul>
        </div>
        <!-- /Bootslider Pagination -->
        <!-- Bootslider Controls -->      
            @*<div class="bs-controls text-center">
            <div class="btn-group">
                <a href="javascript:void(0);" class="btn btn-primary bs-prev">&lt;</a>
                <a href="javascript:void(0);" class="btn btn-primary bs-next">&gt;</a>
            </div>
            </div>*@
  
        <!-- /Bootslider Controls -->

    </div>


    <div class="boot">
        
            <div class="qiye">
                    <a class="boot-a" href="@Url.Action("AllEnterprise","Home",new {area="App",uid=Model.Uid})">企业</a><br>
                    <a class="boot-aa" href="@Url.Action("AllEnterprise","Home",new {area="App",uid=Model.Uid})">@Model.Count 家企业全新资讯</a>
           </div>       

        <div class="mid">
            <div class="mid-left">

                <a class="boot-b" href="">评价</a><br>
                <a class="boot-aa" href="@Url.Action("EvaluateSchool", "Home", new {area = "App", uid = Model.Uid})">学校</a>
                <a class="boot-aa" href="@Url.Action("EvaluateEnterprise", "Home", new {area = "App", uid = Model.Uid })">企业</a>
                <a class="boot-aa" href="@Url.Action("IdentificationTable", "Home", new {area = "App", uid = Model.Uid})">鉴定表</a>
                <a class="boot-aa" href="@Url.Action("PositionEvlTable", "Home", new {area = "App", uid = Model.Uid })">评价表</a>

                    @*<a class="boot-e" href="">记录</a><br>
                    <a class="boot-aa" href="@Url.Action("CheckDiary", "Home", new {area = "App", uid = Model.Uid})">周记</a>
                    <a class="boot-aa" href="@Url.Action("CheckCaseCount", "Home", new {area = "App", uid = Model.Uid})">案例</a>*@

     
            </div>
            <div class="mid-right">
                <div class="pinggu">
                    <a class="boot-e" href="">记录</a><br>
                    <a class="boot-aa" href="@Url.Action("CheckDiary", "Home", new {area = "App", uid = Model.Uid})">周记</a>
                    <a class="boot-aa" href="@Url.Action("CheckCaseCount", "Home", new {area = "App", uid = Model.Uid})">案例</a>
                    <a class="boot-aa" href="@Url.Action("CheckReport", "Home", new {area = "App", uid = Model.Uid})">报告</a>
                    @*<a class="boot-b" href="">评价</a><br>
                    <a class="boot-aa" href="@Url.Action("EvaluateSchool", "Home", new {area = "App", uid = Model.Uid})">学校</a>
                    <a class="boot-aa" href="@Url.Action("EvaluateEnterprise", "Home", new {area = "App", uid = Model.Uid })">企业</a>
                    <a class="boot-aa" href="@Url.Action("IdentificationTable", "Home", new {area = "App", uid = Model.Uid})">鉴定表</a>
                    <a class="boot-aa" href="@Url.Action("PositionEvlTable", "Home", new {area = "App", uid = Model.Uid })">评价表</a>*@
                </div>
                <div class="baogao">
                    <a class="boot-c" href="@Url.Action("CheckStuBatch", "Home", new { area = "App",uid=Model.Uid })">我要报名</a><br>
                    <a class="boot-aa" href="@Url.Action("CheckStuBatch", "Home", new { area = "App",uid=Model.Uid })">@Model.EntCount 家企业正在招聘</a>

                </div>
            </div>

        </div>
        <a href="@Url.Action("PersonalSpace", "Home", new { area = "App",uid=Model.Uid})">
            <div class="zhaoping">
                <a class="boot-d" href="@Url.Action("PersonalSpace", "Home", new { area = "App",uid=Model.Uid})">个人中心</a><br>
                <a class="boot-aa" href="@Url.Action("PersonalSpace", "Home", new { area = "App",uid=Model.Uid})">这里是我的个人信息</a>
            </div>
        </a>
    </div>


    <script src="~/Areas/App/Content/js/jquery-1.10.1.min.js" type="text/javascript"></script>
    <script src="~/Areas/App/Content/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="~/Areas/App/Content/js/jquery.grozav.plugins.min.js" type="text/javascript"></script>
    <script src="~/Areas/App/Content/js/jquery.grozav.bootslider.min.js" type="text/javascript"></script>
    <script>
        $(document).ready(function () {
            var slider = new bootslider('#bootslider', {
                animationIn: "fadeInUp",
                animationOut: "flipOutX",
                timeout: 5000,
                autoplay: true,
                preload: true,
                pauseOnHover: false,
                thumbnails: false,
                pagination: false,
                mousewheel: false,
                keyboard: true,
                touchscreen: true,
                layout: 'fixedheight-center',
                canvas: {
                    width: 1440,
                    height: 720
                }
            });
            slider.init();
        });
    </script>
</body>
</html>




