@model ServicePlatform.Areas.App.ViewModel.Home.Resume_M
<!DOCTYPE html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, height=device-height, initial-scale=1, maximum-scale=1, user-scalable=no" />
    <title>资源下载</title>

    <link href="~/Areas/App/Content/CSS/css.css" rel="stylesheet" />
</head>
<body>
    <div class="main-do">
        <div class="company-head">
            <div class="left">
                <a href="@Url.Action("PracticeOverView","Home",new {uid=Model.uid})">&nbsp;&nbsp;<span><</span>&nbsp;返回</a>
            </div>
            <div class="middle">
                <p>我的简历</p>
            </div>
            <div class="right">
                <p></p>
            </div>
        </div>
        <div class="update">
            <ul>
                @if (Model.Resumes.Count == 0)
                {
                    <li><a href="">很抱歉,你还没有上传简历,请从电脑端上传</a></li>
                }
                else
                {
                    foreach (var item in Model.Resumes)
                    {
                        <li><a href="@item.ResumeURL" download="@item.ResumeName">@item.ResumeName</a></li>
                    }
                }
            </ul>

        </div>
    </div>
</body>