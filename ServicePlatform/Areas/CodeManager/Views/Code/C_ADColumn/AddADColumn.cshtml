@{
    Layout = null;
}

<!DOCTYPE html>



<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>添加广告栏目</title>

    <script src="~/Areas/CodeManager/Content/Easyui/jquery.min.js"></script>
    
    <link href="/Areas/CodeManager/Content/All/css/style.css" rel="stylesheet" type="text/css" />
    
    


</head>
<body>
    <div class="place">
            <span>位置：</span>
            <ul class="placeul">
                <li><a href="#">广告栏目代码管理</a></li>
                <li><a href="#">添加广告栏目</a></li>
            </ul>
        </div>

    <div class="formbody">

            <div class="formtitle"><span>添加广告栏目</span></div>
            

        <form method="post" action="/Code/ReAddADColumn" name="myform" id="myform">

            <ul class="forminfo">




                <li>
                    <label>广告栏目编号:</label>
                    <input type="text" name="ADColumnID" id="ADColumnID" class="dfinput" placeholder="例如：1011（4位数字）">
                </li>


                <li>
                    <label>广告栏目名称:</label>
                    <input type="text" id="ADColumnName" name="ADColumnName" class="dfinput" placeholder="例如：链家网">
                </li>
                
                <li>
                    <label>所属单位选择：</label>
                    <select style="width:100px" name="SybSystem" id="SybSystem" class="dfinput">
                        <option value="学校端">学校端</option>
                        <option value="平台端">平台端</option>
                        <option value="企业端">企业端</option>
                        
                    </select>
                </li>





                <li>
                    <label>&nbsp;</label><input type="submit" value="保  存" class="btn" />

                </li>
            </ul>

        </form>
   
    </div>
     











</body>
</html>
