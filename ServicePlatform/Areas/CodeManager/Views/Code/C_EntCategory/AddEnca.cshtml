@{
    ViewBag.Title = "AddEnca";
}

<!DOCTYPE html>



<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>添加企业类别</title>

   <script src="~/Areas/CodeManager/Content/Easyui/jquery.min.js"></script>
    
    <link href="/Areas/CodeManager/Content/All/css/style.css" rel="stylesheet" type="text/css" />
    
    


</head>
<body>
    <div class="place">
            <span>位置：</span>
            <ul class="placeul">
                <li><a href="#">企业代码管理</a></li>
                <li><a href="#">添加企业类别</a></li>
            </ul>
        </div>

    <div class="formbody">

            <div class="formtitle"><span>添加企业类别</span></div>
            

        <form method="post" action="/Code/ReAddEnca" name="myform" id="myform">

            <ul class="forminfo">




                <li>
                    <label>类别编号:</label>
                    <input type="text" name="EntCategoryID" id="EntCategoryID" class="dfinput" placeholder="例如：0001（4个数字）">
                </li>


                <li>
                    <label>类别名称:</label>
                    <input type="text" id="EntCategoryName" name="EntCategoryName" class="dfinput" placeholder="例如：酒店">
                </li>



                <li>
                    <label>&nbsp;</label><input type="submit" value="保  存" class="btn" />

                </li>
            </ul>

        </form>
   
    </div>
     











</body>
</html>
