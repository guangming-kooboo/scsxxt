@{
    ViewBag.Title = "AddNewsType";
}

<!DOCTYPE html>



<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>添加内新闻类型</title>

   <script src="~/Areas/CodeManager/Content/Easyui/jquery.min.js"></script>
    
    <link href="/Areas/CodeManager/Content/All/css/style.css" rel="stylesheet" type="text/css" />
    
    


</head>
<body>
    <div class="place">
            <span>位置：</span>
            <ul class="placeul">
                <li><a href="#">新闻类型代码管理</a></li>
                <li><a href="#">添加新闻类型</a></li>
            </ul>
        </div>

    <div class="formbody">

            <div class="formtitle"><span>添加新闻类型</span></div>
            

        <form method="post" action="/Code/ReAddNewsType" name="myform" id="myform">

            <ul class="forminfo">




                <li>
                    <label>新闻类型编号:</label>
                    <input type="text" name="TypeID" id="TypeID" class="dfinput" placeholder="例如：01（2个数字）">
                </li>


                <li>
                    <label>新闻类型名称:</label>
                    <input type="text" id="TypeName" name="TypeName" class="dfinput" placeholder="例如：视频新闻">
                </li>



                <li>
                    <label>&nbsp;</label><input type="submit" value="保  存" class="btn" />

                </li>
            </ul>

        </form>
   
    </div>
     











</body>
</html>
