@{
    ViewBag.Title = "updateNewsType";
}

<!DOCTYPE html>



<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>修改新闻类型</title>

    <script src="~/Areas/CodeManager/Content/Easyui/jquery.min.js"></script>
    
    <link href="/Areas/CodeManager/Content/All/css/style.css" rel="stylesheet" type="text/css" />
    
    


</head>
<body>
    <div class="place">
            <span>位置：</span>
            <ul class="placeul">
                <li><a href="#">新闻类型维护</a></li>
                <li><a href="#">修改新闻类型</a></li>
            </ul>
        </div>

    

            <div class="formtitle"><span>修改新闻类型</span></div>
            

    <form method="POST" action="/CodeManager/Code/ReupdateNewsType">

        <ul class="forminfo">




            <li>
                <label>新闻类型编号:</label>
                <input type="text" name="TypeID" id="TypeID" class="dfinput" value="@ViewBag.update.TypeID">
            </li>


            <li>
                <label>新闻类型名称:</label>
                <input type="text" id="TypeName" name="TypeName" class="dfinput" value="@ViewBag.update.TypeName">
            </li>



            <li>

                <label>&nbsp;</label><input type="submit" value="保  存" class="btn">

            </li>
        </ul>



    </form>
     
    

</body>
</html>




