@{
    Layout = null;
}

<!DOCTYPE html>



<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>添加岗位</title>

    <script src="~/Areas/CodeManager/Content/Easyui/jquery.min.js"></script>
    
    <link href="/Areas/CodeManager/Content/All/css/style.css" rel="stylesheet" type="text/css" />
    
    


</head>
<body>
    <div class="place">
            <span>位置：</span>
            <ul class="placeul">
                <li><a href="#">企业代码管理</a></li>
                <li><a href="#">添加岗位</a></li>
            </ul>
        </div>

    <div class="formbody">

            <div class="formtitle"><span>添加岗位</span></div>
            

    <form method="post" action="/Code/ReAddPosition" name="myform" id="myform">
        
           <ul class="forminfo">
               
               
           
       
        <li>
                   <label>岗位编号:</label>
            <input type="text" name="PositionID" id="PositionID" class="dfinput" placeholder="例如：01（2个数字）">
         </li>
            

                <li>
                   <label>岗位名称:</label>
                    <input type="text" id="PositionName" name="PositionName" class="dfinput" placeholder="例如：餐饮部">
         </li>
                
            

              <li>  
                   <label>&nbsp;</label><input type="submit" value="保  存" class="btn"/>
               
            </li>
               </ul>
            
    </form>
   
    </div>
     











</body>
</html>
