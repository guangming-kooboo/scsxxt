@{
    Layout = null;
}

<!DOCTYPE html>



<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>修改岗位</title>

   <script src="~/Areas/CodeManager/Content/Easyui/jquery.min.js"></script>
    
    <link href="/Areas/CodeManager/Content/All/css/style.css" rel="stylesheet" type="text/css" />
    
    


</head>
<body>
    <div class="place">
            <span>位置：</span>
            <ul class="placeul">
                <li><a href="#">企业代码管理</a></li>
                <li><a href="#">修改岗位信息</a></li>
            </ul>
        </div>

    

            <div class="formtitle"><span>修改岗位信息</span></div>
            

    <form method="POST" action="/CodeManager/Code/ReupdatePosition">

        <ul class="forminfo">




            <li>
                <label>岗位编号:</label>
                <input type="text" name="PositionID" id="PositionID" class="dfinput" value="@ViewBag.update.PositionID">
            </li>


            <li>
                <label>岗位名称:</label>
                <input type="text" id="PositionName" name="PositionName" class="dfinput" value="@ViewBag.update.PositionName">
            </li>



            <li>

                <label>&nbsp;</label><input type="submit" value="保  存" class="btn">

            </li>
        </ul>



    </form>
     
    

</body>
</html>

