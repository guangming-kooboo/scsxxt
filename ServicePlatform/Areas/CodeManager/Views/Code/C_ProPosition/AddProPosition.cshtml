@{
    ViewBag.Title = "AddProPosition";
}

<!DOCTYPE html>



<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>添加职称</title>

   <script src="~/Areas/CodeManager/Content/Easyui/jquery.min.js"></script>
    
    <link href="/Areas/CodeManager/Content/All/css/style.css" rel="stylesheet" type="text/css" />
    
    


</head>
<body>
    <div class="place">
            <span>位置：</span>
            <ul class="placeul">
                <li><a href="#">职称代码管理</a></li>
                <li><a href="#">添加职称</a></li>
            </ul>
        </div>

    <div class="formbody">

            <div class="formtitle"><span>添加职称</span></div>
            

    <form method="post" action="/Code/ReAddProPosition" name="myform" id="myform">
        
           <ul class="forminfo">
               
               
           
       
        <li>
                   <label>职称编号:</label>
            <input type="text" name="ProPosID" id="ProPosID" class="dfinput" placeholder="例如：0001（4个数字）">
         </li>
            

                <li>
                   <label>职称名称:</label>
                    <input type="text" id="ProPosName" name="ProPosName" class="dfinput" placeholder="例如：教授">
         </li>
                
            

              <li>  
                   <label>&nbsp;</label><input type="submit" value="保  存" class="btn"/>
               
            </li>
               </ul>
            
    </form>
   
    </div>
     











</body>
</html>

