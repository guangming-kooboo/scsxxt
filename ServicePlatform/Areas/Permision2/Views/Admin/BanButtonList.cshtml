@model ServicePlatform.Areas.Permision2.ViewModels.Admin.BanButtonList_M
@{
    Layout = ViewData["Layout"].ToString();
}

<h2>@ViewBag.Title</h2>
<div class="col-md-12">
<ol class="page-breadcrumb breadcrumb">
        <li><a href="/Permision2/Admin/RoleList?ReportID=Permision_角色列表&Params=@Model.role.RoleID">[角色列表]@Model.role.Name</a> <i class="fa fa-angle-right"></i></li>
</ol>

   </div>
<div class="row">
    <div class="col-lg-10">
        @Html.Hidden("按钮编号", "", new { PlaceHolder = "请输入要查询的按钮编号（支持模糊查询）" })
    </div>
</div>

@*必须添加*@
@Html.Partial("_Report")
<script>
    function Preview() {
        var Params =
            v("按钮编号");

        Query(Params, "@ViewData["Url"]");
    }
</script>