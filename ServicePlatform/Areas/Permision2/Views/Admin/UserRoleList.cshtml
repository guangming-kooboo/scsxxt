@model ServicePlatform.Areas.Permision2.ViewModels.Admin.UserRoleList_M
@{
    Layout = ViewData["Layout"].ToString();
}

<h2>@ViewBag.Title</h2>

<div class="col-md-12">
    <ol class="page-breadcrumb breadcrumb">
        <li><a href="/Permision2/Admin/UserList?ReportID=Permision_用户列表&Params=@Model.user.UserID">[用户列表]@Model.user.NickName</a> <i class="fa fa-angle-right"></i></li>
    </ol>

</div>
<div class="row">
    <div class="col-lg-10">
        @Html.Hidden("用户编号", "", new { PlaceHolder = "请输入要查询的用户编号（支持模糊查询）" })
    </div>
</div>

@*必须添加*@
@Html.Partial("_Report")
<script>
    function Preview() {
        var Params =
            v("用户编号");

        Query(Params, "@ViewData["Url"]");
    }
</script>