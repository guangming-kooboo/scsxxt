@using ServicePlatform.Areas.Permission.Models
@model List<T_FuncBatchOpenSetting>
@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="rightinfo">
    <div class="tools">
        <ul class="toolbar">
            <li><a href="@ViewBag.addLink"><span><img src="/Areas/Platform/Content/Admin/images/t01.png" /></span>添加</a></li>
        </ul>
    </div>

    <table class="tablelist">
        <thead>
            <tr>
                <th><input name="" type="checkbox" value="" checked="checked" /></th>
                <th>批次<i class="sort"><img src="/Areas/Platform/Content/Admin/images/px.gif" /></i></th>
                <th>关闭时间<i class="sort"><img src="/Areas/Platform/Content/Admin/images/px.gif" /></i></th>
                <th>操作</th>
            </tr>
        </thead>
        <tbody>

            @foreach (var item in Model)
            {
                <tr>
                    <td><input name="" type="checkbox" value="" /></td>
                    <td>@item.PracBatchName</td>
                    <td>@item.CloseTime.ToString("yyyy-MM-dd")</td>
                    <td>
                        <a href="Edit?ModuleID=@item.FuncObjID &PracBatchID=@item.PracBatchID">编辑</a> &nbsp;
                        <a href="Delete?ModuleID=@item.FuncObjID &PracBatchID=@item.PracBatchID">删除</a>
                    </td>
                </tr>
            }

        </tbody>
    </table>

</div>
