@model IEnumerable<ServicePlatform.Models.T_Stamps>

@{
    Layout = null;
}

<!DOCTYPE html>



<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>StampDetail</title>

    <link href="~/Areas/DownLoadFile/Content/HeadStyle.css" rel="stylesheet" />

</head>
<body>
    <div id="StampDetailContainer">
        <div id="StampDetailTopic">
            <div class="place">
                <span>位置：</span>
                <ul class="placeul">
                    <li><a href="/SigAndStamp/SSCenter/StampTable">首页</a></li>
                    <li><a href="#">印章详情</a></li>
                </ul>
            </div>
        </div>
        <div id="StampDetailContent">
            @foreach (var s in Model)
            {
                <div id="StampDetail">
                    <div style="margin-top:10px;"><div class="fontSet">印章编号：</div>@s.StampsID</div>
                    <div style="margin-top:10px;"><div class="fontSet">印章标题：</div>@ViewData["thetitle"]</div>
                    <div style="margin-top:10px;"><div class="fontSet">印章发布者：</div>@ViewData["thepublish"]</div>
                    <div style="margin-top:10px;"><div class="fontSet">发布时间：</div>@ViewData["thetime"]</div>
                    <div style="margin-top:10px;"><div class="fontSet">印章种类：</div>@ViewData["TypeName"]</div>
                    <div style="margin-top:10px;"><div class="fontSet">单位名称：</div>@ViewData["UnitName"]</div>
                </div>

                <div id="StampPicBrowser">
                    <div style="margin-top:10px;">
                        <div class="fontSet">
                            印章预览：
                        </div>
                        <div><img src="@s.StampsUrl" /></div>
                    </div>
                </div>
            }
        </div>
    </div>
</body>
</html>

