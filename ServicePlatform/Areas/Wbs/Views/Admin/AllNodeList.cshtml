@model ServicePlatform.Areas.Wbs.ViewModels.Admin.AllNodeList_M
@{
    Layout = ViewData["Layout"].ToString();
}

<h2>@ViewBag.Title</h2>

<div class="col-md-12">
    <ol class="page-breadcrumb breadcrumb">
        <li><a href="/Wbs/Admin/NodeList?ReportID=Wbs_工作量列表&Params=;">[工作量列表]所有工作量</a><i class="fa fa-angle-right"></i></li>
        <li><a href="/Wbs/Admin/NodeList?ReportID=Wbs_工作量列表&Params=@Model.node.NodeID">[工作量列表]@Model.node.NodeName</a><i class="fa fa-angle-right"></i></li>
    </ol>
</div>
<div class="row">
    <div class="col-lg-2">
        <label>任务名称</label>
    </div>
    <div class="col-lg-10">
        @Html.TextBox("任务名称", "", new {PlaceHolder = "请输入要查询的任务名称（支持模糊查询）"})
    </div>
</div>

@*必须添加*@
@Html.Partial("_Report")
<script>
    function Preview() {
        var Params =
            v("任务名称");

        Query(Params, "@ViewData["Url"]");
    }
</script>
