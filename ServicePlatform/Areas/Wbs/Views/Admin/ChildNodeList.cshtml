@model ServicePlatform.Areas.Wbs.ViewModels.Admin.ChildNodeList_M
@{
    Layout = ViewData["Layout"].ToString();
}

<h2>@ViewBag.Title</h2>


    <div class="col-md-12">
        <ol class="page-breadcrumb breadcrumb">   
            @for (var i = 0; i<Model.nodes.Count;i++ )
            {
                <li><a href="/Wbs/Admin/ChildNodeList?ReportID=Wbs_子任务列表&Params=@Model.nodes[i].FatherNodeID">@("["+(i+1)+"级]"+Model.nodes[i].NodeName)</a> <i class="fa fa-angle-right"></i></li>
            }
        </ol>
  
    </div>

<div class="row">

    <div class="col-lg-10">
        @Html.Hidden("父节点", "", new { PlaceHolder = "请输入要查询的父节点（支持模糊查询）" })
    </div>
</div>



@*必须添加*@
@Html.Partial("_Report", new ViewDataDictionary(ViewData)
{
    {"showDeafultButton",false }
})
<script>
    function Preview() {
        var Params =
            v("父节点");

        Query(Params, "@ViewData["Url"]");
    }
</script>
