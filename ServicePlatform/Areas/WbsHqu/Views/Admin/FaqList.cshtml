@using Qx.Tools.Scsxxt.Web.Mvc.Html
@model ServicePlatform.Areas.WbsHqu.ViewModels.FAQ.FAQList_M
@{
    Layout = ViewData["Layout"].ToString();
}
<label>

    FAQ列表:&nbsp;
    @Html.TextBox("FAQ列表", "", new {@class = "form-control", PlaceHolder = "请输入要查询的文章标题"})
    @Html.SelectFor(o => o.stateId, Model._FAQState)

</label>


<script>
    function Search() {
        var Params = v("FAQ列表");
        Params = Params==undefined ? ";" : Params;
        var Params1 = $("#stateId").val();
        Params1 = Params1 == undefined ? ";" : Params1;
        return Params +";"+ Params1;
    }


</script>