<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>@ViewData["Title"]</title>
    <script src="/assets/plugins/jquery-1.10.2.min.js" type="text/javascript"></script>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @*补充*@

    <link href="@Url.Content("~/bower_components/bootstrap/dist/js/bootstrap.min.js")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content(" ~/Scripts/respond.js")" rel="stylesheet" type="text/css" />

   
    <link href="@Url.Content("~/bower_components/bootstrap/dist/css/bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/bower_components/metisMenu/dist/metisMenu.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/timeline.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/sb-admin-2.css")" rel="stylesheet" type="text/css" />
    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/css/bootstrap-datetimepicker.min.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
  
    <!-- DataTables CSS -->
    <link href="@Url.Content("~/bower_components/datatables-plugins/integration/bootstrap/3/dataTables.bootstrap.css")" rel="stylesheet" type="text/css" />
    <!-- DataTables Responsive CSS -->
    <link href="@Url.Content("~/bower_components/datatables-responsive/css/dataTables.responsive.css")" rel="stylesheet" type="text/css" />

    <link href="@Url.Content("~/bower_components/morrisjs/morris.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/bower_components/font-awesome/css/font-awesome.min.css")" rel="stylesheet" type="text/css" />

    <!-- jQuery -->
    @Scripts.Render("~/bundles/jquery")
    <script src="@Url.Content("~/bower_components/jquery/dist/jquery.min.js")"></script>
    @Scripts.Render("~/bundles/bootstrap")
    <script src="@Url.Content("~/bower_components/metisMenu/dist/metisMenu.min.js")"></script>

</head>
<body>
    <div id="wrapper">
        <!-- Navigation -->
        @*@Html.Action("Index", "Navbar",new {area=""})*@

        <div id="container">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12">
                        <h1 class="page-header">@ViewData["Title"] <button class="btn btn-primary back" onclick="back()"> 上一级</button>
                        </h1>
                    </div>
                    <!-- /.col-lg-12 -->
                </div>
                <!-- /.row -->
            </div>

            <div id="searchContent">
                @RenderBody()
            </div>
            @Html.Partial("_Report_Scsxxt")
        </div>
        <!-- /#page-wrapper -->
    </div>
    <!-- /#wrapper -->
    <!-- DataTables JavaScript -->
    <script src="@Url.Content("~/bower_components/raphael/raphael-min.js")"></script>

    <!-- DataTables JavaScript -->
    <script src="@Url.Content("~/bower_components/datatables/media/js/jquery.dataTables.min.js")"></script>
    <script src="@Url.Content("~/bower_components/datatables-plugins/integration/bootstrap/3/dataTables.bootstrap.min.js")"></script>

    <!-- Custom Theme JavaScript -->
    <script src="@Url.Content("~/Scripts/sb-admin-2.js")"></script>

<!-- Page-Level Demo Scripts - Tables - Use for reference -->
    <script src="~/assets/plugins/data-tables/DT_bootstrap.js"></script>
<link href="~/assets/plugins/data-tables/DT_bootstrap.css" rel="stylesheet"/>

        <script src="~/assets/plugins/bootstrap-datetimepicker/js/bootstrap-datetimepicker.min.js"></script>

<link href="~/assets/plugins/bootstrap-datetimepicker/css/datetimepicker.css" rel="stylesheet"/>
    <script src="/assets/scripts/app.js"></script>

<script type="text/javascript">
    jQuery(document).ready(function() {
        App.init();
    });

    $(".form_datetime").datetimepicker({
        autoclose: true,
        isRTL: App.isRTL(),
        format: "yyyy-MM-dd hh:ii",
        pickerPosition: (App.isRTL() ? "bottom-right" : "bottom-left")
    });
    $(".back").click(back);

    //调整搜索内容位置
    var old = $("#searchContainer").html();
    $("#searchContainer").html($("#searchContent").html() + old);
    //alert($("#searchContent").html());
    $("#searchContent").remove();

    //调整表格下方的内容
    if ($("#table-down").val() != undefined) {
        var old = $("#table-down-Container").html();
        $("#table-down-Container").html($("#table-down").html() + old);
        //alert($("#searchContent").html());
        $("#table-down").remove();
    }

    function back() {
        history.back();
    }
</script>

    <script type="text/javascript">
    $(document).ready(function() {
        $('#dataTables-example').DataTable({
            responsive: true
        });

      


    });
    </script>
   
   
  
 
    @RenderSection("scripts", required: false)
</body>
</html>

